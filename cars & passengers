cars=100
space_in_a_car=4.0
drivers=30
passengers=90
cars_not_driven=cars-drivers
cars_driven=drivers
carpool_capacity=cars_driven * space_in_a_car
average_passengers_per_car=passengers/cars_driven


print('there are', cars, 'cars in this thang!!!')
print('if you aint riding', drivers,'deep')
print('where the big Xboxes at', cars_not_driven, 'rolls')
print('we can move', carpool_capacity, 'tooka packs')
print('no scope 360',passengers, 'on me!!!')
print('we need more cyber gangstas'
,average_passengers_per_car, 'in Texasss')
